{
  "name": "firefox-profiler",
  "publisher": "nisargjhaveri",
  "displayName": "Firefox Profiler",
  "description": "",
  "version": "0.0.1",
  "repository": "https://github.com/nisargjhaveri/vscode-firefox-profiler",
  "license": "MIT",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "firefox-profiler.viewer",
        "displayName": "Firefox Profiler",
        "selector": [
          {
            "filenamePattern": "{*.trace,*.trace.gz}"
          },
          {
            "filenamePattern": "{*.cpuprofile,*.cpuprofile.gz}"
          },
          {
            "filenamePattern": "{*.perf,*.perf.gz}"
          },
          {
            "filenamePattern": "{perf.json,*.perf.json,*.json.gz}"
          },
          {
            "filenamePattern": "{*.perf.txt,*.perf.txt.gz}"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "firefox-profiler.viewer.optional",
        "displayName": "Firefox Profiler (Optional)",
        "selector": [
          {
            "filenamePattern": "{*.json,*.txt,*.gz,*}"
          }
        ],
        "priority": "option"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run check-types && yarn run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "yarn run check-types && yarn run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "esbuild": "^0.24.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
